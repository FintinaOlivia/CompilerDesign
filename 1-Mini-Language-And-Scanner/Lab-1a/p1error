int max(int firstNumber, int secondNumber, int thirdNumber) {
    int max = +0; // invalid integer
string message = “The max number is; //error: no closing “ for string


    if (firstNumber <= thirdNumber && secondNumber <= thirdNumber) {     
        max = thirdNumber;                                              
    } else if (firstNumber <= secondNumber && thirdNumber <= secondNumber) {
        max = secondNumber;
    } else {
        max = firstNumber;
    }
	print(message, max);
}
